node blur (float strength, int img) {
  // start variable declaration region
  float pi = 3.14;
  string swap = "RAM";
  // end variable declaration region

  // start function declaration region
  fun int bpixarea (int region) {
    return region * strength;
  }
  // end function declaration region

  // expression region
  img_region = img + 42;
  return bpixarea(img_region);
  // end expression region
}

